{
	"name": "My Extension",
	"version": "1.0",
	"manifest_version": 2,
	"description": "This is a short description for our Chrome extension.",
	
	
	"permissions": [
		"https://*/*"
	  ],
	"content_scripts": [
	{
		"matches": ["https://*/*", "http://*/*", "file:///*/*"],
		"js": ["jquery.js","content.js"],
		"run_at": "document_end"
	}
	],
	

	"background": {
		"scripts": [
		  "jquery.js",
		  "background.js"
		]
	  },

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "mypopup.html",
		"omnibox": { "keyword" : "yeah" }
	}
}